enable
  conf term
    hostname S2
    int vlan 20
      ip address 169.254.0.2 255.255.0.0
      no shutdown

    int Fa0/1
      switchport mode access
      switchport access vlan 20
    int Fa1/1
      switchport mode access
      switchport access vlan 20
    int Fa2/1
      switchport mode access
      switchport access vlan 20
    int Fa3/1
      switchport mode access
      switchport access vlan 20
      exit
    exit

  copy running-config startup-config
    startup-config
