enable
  conf term
    hostname S3
    int vlan 30
      ip address 10.10.30.254 255.255.255.0
      no shutdown

    int Fa0/1
      switchport mode access
      switchport access vlan 30
    int Fa1/1
      switchport mode access
      switchport access vlan 30
    int Fa2/1
      switchport mode access
      switchport access vlan 30
    int Fa3/1
      switchport mode access
      switchport access vlan 30
      exit
    exit

  copy running-config startup-config
    startup-config
